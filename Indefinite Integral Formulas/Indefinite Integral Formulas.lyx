\begin_layout Standard
(I)sin/cos type 
\begin_inset Formula $\sqrt{a^{2}-x^{2}}$
\end_inset


\end_layout

\begin_layout Standard
let 
\begin_inset Formula $x=a\times sin\theta$
\end_inset

, 
\begin_inset Formula $dx=a\times cos\theta$
\end_inset

; or 
\begin_inset Formula $x=a\times cos\theta$
\end_inset

, 
\begin_inset Formula $dx=-asin\theta d\theta$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $sin^{2}\theta+cos^{2}\theta=1$
\end_inset


\end_layout

\begin_layout Standard
(II)sec/csc type 
\begin_inset Formula $\sqrt{x^{2}-a^{2}}$
\end_inset


\end_layout

\begin_layout Standard
let 
\begin_inset Formula $x=a\times sec\theta$
\end_inset

, 
\begin_inset Formula $dx=a\times sec\theta tan\theta d\theta$
\end_inset

; or 
\begin_inset Formula $x=a\times csc\theta$
\end_inset

, 
\begin_inset Formula $dx=-a\times csc\theta cot\theta$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $sec^{2}\theta=1+tan^{2}\theta$
\end_inset


\end_layout

\begin_layout Standard
(III)tan/cot type 
\begin_inset Formula $\sqrt{x^{2}+a^{2}}$
\end_inset


\end_layout

\begin_layout Standard
let 
\begin_inset Formula $x=a\times tan\theta$
\end_inset

, 
\begin_inset Formula $dx=a\frac{1}{cos^{2}\theta}$
\end_inset

; or 
\begin_inset Formula $x=a\times cot\theta$
\end_inset

, 
\begin_inset Formula $dx=-a\frac{1}{sin^{2}\theta}$
\end_inset


\end_layout

