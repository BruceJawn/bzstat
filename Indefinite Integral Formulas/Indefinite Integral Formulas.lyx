\end_inset


\end_layout

\begin_layout Standard
Pf: left side 
\begin_inset Formula $=\frac{1}{a}\int cos(bx)de^{ax}=\frac{1}{a}(cos(bx)e^{ax}+\int be^{ax}sin(bx)dx)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\int be^{ax}sin(bx)dx=\frac{b}{a}\int sin(bx)de^{ax}=\frac{b}{a}(sin(bx)e^{ax}-\int e^{ax}dsin(bx))$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $=\frac{b}{a}(sin(bx)e^{ax}-b\int e^{ax}cos(bx)dx)$
\end_inset


\end_layout

\begin_layout Standard
so 
\begin_inset Formula $a\int e^{ax}cos(bx)dx=cos(bx)e^{ax}+\frac{b}{a}sin(bx)e^{ax}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\int e^{ax}cos(bx)dx=\frac{acos(bx)e^{ax}+bsin(bx)e^{ax}}{a^{2}+b^{2}}$
\end_inset

.
\end_layout

