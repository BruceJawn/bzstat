\begin_layout Standard

\series bold
25.

\series default
 
\begin_inset Formula $\int e^{ax}sin(bx)dx=\frac{e^{ax}(asin(bx)-bcos(bx))}{a^{2}+b^{2}}+c$
\end_inset


\end_layout

\begin_layout Standard
Pf: integral by parts, left side 
\end_layout

\begin_layout Standard
\begin_inset Formula $=\frac{1}{a}\int sin(bx)de^{ax}=\frac{1}{a}(sin(bx)e^{ax}-\int be^{ax}cos(bx)dx)$
\end_inset


\end_layout

\begin_layout Standard
and 
\begin_inset Formula $\int be^{ax}cos(bx)dx=\frac{b}{a}\int cos(bx)de^{ax}=\frac{b}{a}(cos(bx)e^{ax}-\int e^{ax}dcos(bx))$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $=\frac{b}{a}(cos(bx)e^{ax}+b\int e^{ax}sin(bx)dx)$
\end_inset


\end_layout

\begin_layout Standard
so 
\begin_inset Formula $\int e^{ax}sin(bx)dx=\frac{1}{a}(sin(bx)e^{ax}-\frac{b}{a}(cos(bx)e^{ax}+b\int e^{ax}sin(bx)dx))$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $a\int e^{ax}sin(bx)dx=sin(bx)e^{ax}-\frac{b}{a}cos(bx)e^{ax}-\frac{b^{2}}{a}\int e^{ax}sin(bx)dx$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $(a+\frac{b^{2}}{a})\int e^{ax}sin(bx)dx=sin(bx)e^{ax}-\frac{b}{a}cos(bx)e^{ax}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\int e^{ax}sin(bx)dx=\frac{1}{a^{2}+b^{2}}(asin(bx)e^{ax}-bcos(bx)e^{ax})+c$
\end_inset

.
\end_layout

