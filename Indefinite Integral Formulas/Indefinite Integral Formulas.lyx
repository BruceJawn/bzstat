\begin_layout Standard

\series bold
27.

\series default
 
\begin_inset Formula $\int\frac{1}{(x^{2}+a^{2})}dx=\frac{1}{2a^{2}}\frac{x}{x^{2}+a^{2}}+\frac{1}{2a^{3}}arctan\frac{x}{a}+c$
\end_inset


\end_layout

\begin_layout Standard
Pf: let 
\begin_inset Formula $x=a\times tan\theta$
\end_inset

, 
\begin_inset Formula $x^{2}+a^{2}=\frac{1}{cos^{2}\theta}a^{2}$
\end_inset

,
\end_layout

\begin_layout Standard
left side 
\begin_inset Formula $=\int\frac{\frac{a}{cos^{2}\theta}}{\frac{1}{cos^{4}\theta}a^{4}}d\theta=\frac{1}{a^{3}}\int cos^{2}\theta d\theta=\frac{1}{2a^{3}}\int\frac{1+cos2\theta}{2}d2\theta$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $=\frac{1}{4a^{3}}(2\theta+sin2\theta)$
\end_inset


\end_layout

\begin_layout Standard
note that 
\begin_inset Formula $\theta=arctan\frac{x}{a}$
\end_inset

, 
\begin_inset Formula $sin2\theta=2sin\theta cos\theta=\frac{2ax}{a^{2}+x^{2}}$
\end_inset


\end_layout

\begin_layout Standard
so left side 
\begin_inset Formula $=\frac{1}{4a^{3}}(2arctan\frac{x}{a}+\frac{2ax}{a^{2}+x^{2}})+c$
\end_inset

.
\end_layout

